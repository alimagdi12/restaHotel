    <!-- ##### Milestones Area Start ##### -->
    <section class="our-milestones section-padding-100-0 bg-img bg-overlay bg-fixed" style="background-image: url(./R-IMG/background/11.jpg);">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-12 col-lg-8">
                    <div class="section-heading text-center white">
                        <div class="line-"></div>
                        <h2>Our Milestones</h2>
                        <p> Featuring a stunning location near the Mediterranean Sea, Resta Hotel overlooks passing boats and ships right at the entrance of the Suez Canal. It features a contemporary building and an outdoor pool.
                            
                        </p>
                    </div>
                </div>
            </div>

            <div class="row">

                <!-- Single Cool Facts -->
                <div class="col-12 col-sm-6 col-lg-3">
                    <div class="single-cool-fact mb-100 wow fadeInUp" data-wow-delay="300ms">
                        <div class="scf-text">
                            <i class="icon-cocktail-1"></i>
                            <h2><span class="counter">2</span></h2>
                            <p>View</p>
                        </div>
                    </div>
                </div>

                <!-- Single Cool Facts -->
                <div class="col-12 col-sm-6 col-lg-3">
                    <div class="single-cool-fact mb-100 wow fadeInUp" data-wow-delay="500ms">
                        <div class="scf-text">
                            <i class="icon-swimming-pool"></i>
                            <h2><span class="counter">1</span></h2>
                            <p>Pool</p>
                        </div>
                    </div>
                </div>

                <!-- Single Cool Facts -->
                <div class="col-12 col-sm-6 col-lg-3">
                    <div class="single-cool-fact mb-100 wow fadeInUp" data-wow-delay="700ms">
                        <div class="scf-text">
                            <i class="icon-resort"></i>
                            <h2><span class="counter">110</span></h2>
                            <p>Rooms</p>
                        </div>
                    </div>
                </div>

                <!-- Single Cool Facts -->
                <div class="col-12 col-sm-6 col-lg-3">
                    <div class="single-cool-fact mb-100 wow fadeInUp" data-wow-delay="900ms">
                        <div class="scf-text">
                            <i class="icon-restaurant"></i>
                            <h2><span class="counter">2</span></h2>
                            <p>Restaurant & Cafe </p>
                        </div>
                    </div>
                </div>

            </div>
        </div>
    </section>
    <!-- ##### Milestones Area End ##### -->

    <!-- ##### Hotels Area Start ##### -->
    <section class="our-hotels-area section-padding-100-0">
        <div class="container">
            <div class="row">
                <div class="col-12">
                    <div class="section-heading text-center">
                        <div class="line-"></div>
                        <h2 style="color: #85714D;">Our Hotel</h2>
                    </div>
                </div>
            </div>

            <div class="row justify-content-center">
                <!-- Single Hotel Info -->
                <div class="col-12 col-md-6 col-lg-4">
                    <div class="single-hotel-info mb-100">
                        <div class="hotel-info-text">
                            <h6><span class="fa fa-check"></span> Swimming pool, Pool/Beach towels, Outdoor pool (seasonal), Outdoor pool</h6>
                            <h6><span class="fa fa-check"></span>Fruit, Bottle of water, Breakfast in the room, Bar, Restaurant </h6>
                            <h6><span class="fa fa-check"></span>Free!WiFi is available in all areas and is free of charge.</h6>
                        </div>
                        <div class="hotel-img">
                            <img src="./R-IMG/background/resta-port-said-hotel_888888.jpg" alt="" style="height: 380px ;">
                        </div>
                    </div>
                </div>
                <!-- Single Hotel Info -->
                <div class="col-12 col-md-6 col-lg-4">
                    <div class="single-hotel-info mb-100">
                        <div class="hotel-info-text">
                            <h6><span class="fa fa-check"></span> Free!Free private parking is available on site (reservation is not needed).</h6>
                            <h6><span class="fa fa-check"></span> ATM on site, Baggage storage, Currency exchange, 24-hour front desk</h6>
                            <h6><span class="fa fa-check"></span>Daily housekeeping, Ironing service,Dry cleaning, Laundry    </h6>
                        </div>
                        <div class="hotel-img">
                            <img src="./R-IMG/1.jpg" alt="" style="height: 380px ;">
                        </div>
                    </div>
                </div>
                <!-- Single Hotel Info -->
                <div class="col-12 col-md-6 col-lg-4">
                    <div class="single-hotel-info mb-100">
                        <div class="hotel-info-text">
                            <h6><span class="fa fa-check"></span> Fax/Photocopying, Meeting/Banquet facilities</h6>
                            <h6><span class="fa fa-check"></span> Air conditioning, Safe, Elevator, Hair/Beauty salon, Non-smoking rooms, Room service              </h6>
                            <h6><span class="fa fa-check"></span>Languages Spoken	English, Arabic  </h6>
                        </div>
                        <div class="hotel-img">
                            <img src="./R-IMG/6.jpg" alt="" style="height: 380px ;">
                        </div>
                    </div>
                </div>
            </div>
        </div>
