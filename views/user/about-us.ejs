                        <div class="section-heading text-center">
                            <div class="line-"></div>
                            <h2 style="color:#85714D ;">What Clients Say</h2>
                        </div>
                        
                        <!-- Testimonial Slides -->
                        <div class="testimonial-slides owl-carousel" style="font-size: 18px;">
                            <% if (opin.length > 0) { %>
                                <% for (let opinions of opin) { %>
                            <!-- Single Testimonial -->
                            <div class="single-testimonial" style="font-size: 18px;">
                                <p><%= opinions.opinion %></p>
                                <h6><%= opinions.name %>, <span><%= opinions.job %></span></h6>
                                <img src="<%= opinions.image %>" alt="">
                            </div>
                            
                            <% } %>
                            <% } %>
                            
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- ##### Testimonial Area End ##### -->


